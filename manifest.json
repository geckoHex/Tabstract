{
  "manifest_version": 3,
  "name": "Tabstract - New Tab Page",
  "version": "1.0.0",
  "description": "A customizable new tab page with a search bar",
  "permissions": [
    "storage"
  ],
  "host_permissions": [
    "https://www.google.com/*"
  ],
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  }
}
