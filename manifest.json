{
  "manifest_version": 3,
  "name": "Tabstract",
  "version": "1.1.1",
  "description": "A delightful, customizable, and efficient new-tab page.",
  "permissions": [
    "storage"
  ],
  "host_permissions": [
    "https://www.google.com/*"
  ],
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "icons": {
  "16": "public/ext-assets/icon16.png",
  "32": "public/ext-assets/icon32.png",
  "48": "public/ext-assets/icon48.png",
  "128": "public/ext-assets/icon128.png"
}
}
