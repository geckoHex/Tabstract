{
  "manifest_version": 3,
  "name": "Tabstract - New Tab Page",
  "version": "1.0.0",
  "description": "A customizable new tab page with a search bar",
  "permissions": [
    "chrome://favicon/"
  ],
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "icons": {
    "16": "images/icon-16.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "action": {
    "default_icon": "images/icon-16.png"
  }
}
